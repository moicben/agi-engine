{
  "workers/vision": {
    "detect": {
      "params": ["image", "query", "annotate?", "outPath?", "threshold?"],
      "produces": ["coords", "score", "annotated_path"],
      "tags": ["vision", "zero-shot", "owlv2"],
      "timeouts": { "default_ms": 60000 }
    }
  },
  "workers/interact": {
    "orchestrate": {
      "params": ["decision", "context?"],
      "produces": ["success", "action", "data"],
      "tags": ["gui", "input"],
      "timeouts": { "default_ms": 30000 }
    }
  },
  "workers/qa": {
    "answer": {
      "params": ["question", "context?"],
      "produces": ["answer"],
      "tags": ["qa", "llm"],
      "timeouts": { "default_ms": 30000 }
    }
  },
  "workers/web": {
    "search": {
      "params": ["query", "maxResults?"],
      "produces": ["results"],
      "tags": ["web", "search"],
      "timeouts": { "default_ms": 45000 }
    }
  },
  "workers/developer": {
    "integrate": {
      "params": ["spec", "options?"],
      "produces": ["applied"],
      "tags": ["code", "integrate"],
      "timeouts": { "default_ms": 120000 }
    },
    "generateCode": {
      "params": ["spec"],
      "produces": ["edits"],
      "tags": ["code", "plan"],
      "timeouts": { "default_ms": 30000 }
    }
  }
  ,
  "workers/editor": {
    "applyEdits": {
      "params": ["edits"],
      "produces": ["applied"],
      "tags": ["code", "edit"],
      "timeouts": { "default_ms": 60000 }
    }
  }
}
